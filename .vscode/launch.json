    "version": "0.2.0",
    "configurations": [
         {
            "name": "Chrome",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "toolArgs": ["-d","chrome"]
            // "deviceId": "chrome"
        },
        {
            "name": "WebServer",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": ["--release","-d","web-server"]
        },
        {
            "name": "WebServer-debug",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": ["-d","web-server"]
        },
        {
            "name": "qarin_mvvm",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "qarin_mvvm (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "qarin_mvvm (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}